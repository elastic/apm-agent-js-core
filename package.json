{
  "name": "elastic-apm-js-core",
  "version": "2.0.0",
  "description": "Elastic apm core",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/apm-agent-js-core.git"
  },
  "scripts": {
    "prepare-patch": "sh ./dev-utils/prepare-release patch && npm run push-release",
    "prepare-minor": "sh ./dev-utils/prepare-release minor && npm run push-release",
    "prepare-major": "sh ./dev-utils/prepare-release major && npm run push-release",
    "push-release": "git push origin master --follow-tags && npm publish",
    "changelog": "node node_modules/conventional-changelog-cli/cli.js  -p angular -i CHANGELOG.md -s",
    "karma": "node node_modules/karma/bin/karma start",
    "karma-bench": "node node_modules/karma/bin/karma start karma.bench.conf.js",
    "karma-coverage": "node node_modules/karma/bin/karma start --coverage",
    "runUnitTests": "node ./dev-utils/run-script.js runUnitTests",
    "test": "node node_modules/standard/bin/cmd.js src/**/* && npm run runUnitTests",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "files": [
    "src/**/*.js",
    "test/**/*.js",
    "dev-utils/**/*"
  ],
  "dependencies": {
    "error-stack-parser": "^1.3.5",
    "stack-generator": "^1.0.7",
    "url-parse": "1.4.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "2.1.4",
    "conventional-changelog-cli": "1.3.22",
    "es6-promise": "^4.2.4",
    "express": "4.16.3",
    "istanbul": "^0.4.4",
    "jasmine": "^2.8.0",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "3.0.0",
    "karma-benchmark": "1.0.0",
    "karma-benchmark-json-reporter": "1.0.1",
    "karma-benchmark-reporter": "0.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-failed-reporter": "0.0.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-sauce-launcher": "1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^2.0.4",
    "prettier-standard": "8.0.1",
    "sauce-connect-launcher": "1.2.4",
    "selenium-standalone": "6.15.2",
    "standard": "^10.0.3",
    "wdio-jasmine-framework": "^0.3.2",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "4.13.2",
    "webpack": "^3.6.0"
  },
  "standard": {
    "globals": [
      "jasmine",
      "describe",
      "beforeEach",
      "afterEach",
      "it",
      "xit",
      "expect",
      "fail",
      "spyOn",
      "browser",
      "xdescribe"
    ]
  }
}
